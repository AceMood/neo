  "maintainers": [
    {
      "name": "AceMood",
      "email": "zmike86@gmail.com"
    }
  ],
  "dependencies": {
    "et-util-logger": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "codacy-coverage": "^1.1.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0"
  }
