{
  "name": "neo-core",
  "version": "0.5.6",
  "description": "Facade of FrontEnd Static Resource System.",
  "main": "neo.js",
  "engines": {
    "node": ">= 1.0.0"
  },
  "scripts": {
    "test": "NODE_ENV=d4d9d176ebf14d3a8938295efac23bfd YOURPACKAGE_COVERAGE=1 ./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/.bin/codacy-coverage"
  },
  "config": {
    "blanket": {
      "pattern": ["lib"],
      "data-cover-never": [
        "node_modules",
        "test",
        "__test_data__",
        "__test_helpers",
        "__legacy_code__"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AceMood/neo.git"
  },
  "keywords": [
    "Static Resource Management",
    "Front End",
    "ResourceMap"
  ],
  "author": "Facebook.Inc, AceMood, Songjin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AceMood/neo/issues"
  },
  "homepage": "https://github.com/AceMood/neo",
  "dependencies": {
    "et-util-logger": "^1.1.0"
  },
  "devDependencies": {
    "blanket": "^1.2.1",
    "chai": "^3.4.1",
    "codacy-coverage": "^1.1.3",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0"
  }
}
